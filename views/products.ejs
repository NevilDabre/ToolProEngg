<%- include layouts/header.ejs %>



    <section id="products-heading" class="products">
        <div class="products-container Container text-center">
            <h2 class="margin" style="color:#dd4b39;">Products & Services!</h2>
            <p class="indent" style="color:#7f0cad;">EVERYTHING YOU NEED FOR PRECISION TOOLING AND MORE. </p>
        </div>
    </section>

    <section id="products-popUp" class="products-popUp">

        <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="gridSystemModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="gridSystemModalLabel">Send Inquiry!</h4>
                        <div class='col-md-12'>
                            <div class='thumbnail'>
                                <div class='products-image'>
                                    <img id="inquiryImage" class='products-image' alt='Inquiry Image'>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-body">
                        <div class="col-sm-12">
                            <div class="form-group col-sm-14">
                                <label for="inquiryEmail"> Your Email: </label>
                                <input type="email" class="form-control" id="inquiryEmail" placeholder="Enter Email">
                            </div>
                            <div class="form-group col-sm-14">
                                <label for="inquiryPhone"> Your Phone: </label>
                                <input type="phone" class="form-control" id="inquiryPhone" placeholder="Enter Phone Number">
                            </div>
                            <div class="form-group  col-sm-14">
                                <label for="inquiryMessage"> Your Message: </label>
                                <textarea class="form-control" rows="5" id="inquiryMessage"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">Send</button>
                    </div>
                </div>
            </div>
        </div>
    </section>

